*Given an integer x, return true if x is a palindrome, and false otherwise.



class Solution {
    func isPalindrome(_ x: Int) -> Bool {
    //turn our Int to string
        let s = String(x)
    //reverse it    
        let stringReversed = String(s.reversed())
    //compare and return Bool    
        return s == stringReversed
    }
   
}

let solution = Solution()

print(solution.isPalindrome(101))
print(solution.isPalindrome(134))

